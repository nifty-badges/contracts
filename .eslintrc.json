{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "plugins": [
    "prettier",
    "import",
    "@typescript-eslint",
    "jest"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "@typescript-eslint/no-unused-vars": "warn",
    "import/default": "off",
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {
          "{}": false
        }
      }
    ]
  }
}